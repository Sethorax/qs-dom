{
  "name": "qs-dom",
  "version": "0.2.1",
  "description": "Fast and tiny library to help with traversing and manipulating the DOM.",
  "main": "dist/qs-dom.umd.js",
  "module": "dist/qs-dom.es5.js",
  "types": "dist/types/qs-dom.d.ts",
  "repository": "https://github.com/Sethorax/qs-dom.git",
  "author": "Sethorax <info@sethorax.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/jsdom": "^11.0.6",
    "coveralls": "^3.0.2",
    "jest": "^23.4.2",
    "jsdom": "^11.12.0",
    "lodash.camelcase": "^4.3.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.63.4",
    "rollup-plugin-commonjs": "^9.1.4",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.16.1",
    "ts-jest": "^23.0.1",
    "typescript": "^2.9.2"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "test:prod": "npm run test -- --coverage --no-cache",
    "report-coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "verbose": true,
    "testURL": "http://localhost/",
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    },
    "collectCoverage": true
  }
}
